        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="IngressClassName is the name of an IngressClass cluster resource. Ingress controller implementations use this field to know whether they should be serving this Ingress resource."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
