          <details key="documentation" value="List of containers belonging to the pod."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="initContainers" upperBound="-1"
          eType="#//workloads/Container" containment="true">
