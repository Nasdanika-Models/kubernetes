          <details key="documentation" value="Restart policy for all containers within the pod. One of Always, OnFailure, Never. Default to Always."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="serviceAccountName" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
